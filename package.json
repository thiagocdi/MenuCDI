    "publish": "electron-forge publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thiagocdi/MenuCDI"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "node-stream-zip": "^1.15.0",

{
	"name": "menucdi",
	"version": "1.0.21",
	"description": "Menu CDI Softwares Launcher",
	"main": "main.js",
	"author": "CDI Inform√°tica e Assessoria Ltda",
	"license": "MIT",
	"type": "commonjs",
	"scripts": {
		"start": "electron .",
		"build": "electron-builder",
		"build-legacy": "electron-builder --win --x64 --config.electronVersion=13.6.9 --config.directories.output=dist/legacy",
		"startF": "electron-forge start",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"publish": "electron-forge publish",
		"publish___naofuncionou": "electron-builder --win --publish always"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/thiagocdi/MenuCDI"
	},
	"dependencies": {
		"axios": "^1.6.0",
		"node-stream-zip": "^1.15.0",
		"update-electron-app": "^3.1.1",
		"win-version-info": "^6.0.1"
	},
	"devDependencies": {
		"@electron-forge/cli": "^7.10.2",
		"@electron-forge/maker-squirrel": "^7.10.2",
		"@electron-forge/publisher-github": "^7.10.2",
		"electron": "22.3.27",
		"electron-builder": "23.6.0",
		"electron-builder-squirrel-windows": "^23.6.0",
		"electron-packager": "17.1.2"
	},
	"build": {
		"appId": "com.cdi.menu",
		"productName": "MenuCDI",
		"directories": {
			"output": "dist"
		},
		"asar": true,
		"files": [
			"main.js",
			"preload.js",
			"index.html",
			"login.html",
			"assets/**",
			"node_modules/**",
			"package.json"
		],
		"win": {
			"target": [
				"squirrel"
			],
			"icon": "assets/images/icon.ico"
		},
		"publish": [
			{
				"provider": "github",
				"owner": "thiagocdi",
				"repo": "MenuCDI",
				"releaseType": "release"
			}
		]
	}
}

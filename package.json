    "electron": "22.3.27",
    "electron-builder": "23.6.0",
    "electron-packager": "17.1.2"
  },
  "build": {
    "appId": "com.cdi.menu",
    "productName": "MenuCDI",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "portable",
      "icon": "assets/images/icon.ico"
